  }
  .s2 {
    bottom: 60px;
    right: 100px;
    width: 40px;
    rotate: 90deg;
  }
  .abs {
    position: absolute;
  }
  .title {
    text-size-adjust: auto;
    transform-origin: center center;
    font-size: 5em;
    line-height: 50px;
    text-transform: uppercase;
    text-align: center;
  }
  .brSection {
    height: 2px;
    width: calc(100% - 40px);
    margin: 40px 20px;
    background-color: #000;
  }
  .nextSection {
    width: calc(100% - 40px);
